{
  "extends": [
    "next/core-web-vitals"
  ],
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/display-name": "off",
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error"
    ],
    "unused-imports/no-unused-imports": "error"
  }
}